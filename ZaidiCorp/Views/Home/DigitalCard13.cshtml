            $('.ol_h').click(function () {

                if (!$(this).next('.ol_v').is(':visible')) {
                    $(this).find('.p').text('-');
                }
                else {
                    $(this).find('.p').text('+');
                }
            });

            // Share Menu
            var Menu = {
                body: $('.share-menu'),
                button: $('.share-button'),
                tools: $('.share-tools')
            };
            Menu.button.click(function () {
                Menu.body.toggleClass('share-menu--closed');
                Menu.body.toggleClass('share-menu--open');
                Menu.tools.toggleClass('share-tools--visible');
                Menu.tools.toggleClass('share-tools--hidden');
            });

        });
    </script>

    <script src="https://www.google.com/recaptcha/api.js" async defer></script>

    <script>
        $(document).ready(function () {
            $("#register").validate({
