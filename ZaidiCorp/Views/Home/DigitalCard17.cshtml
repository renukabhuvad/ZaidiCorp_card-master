                <script>
                 function handleWhatsappShare(e) {
                     const {value} = document.getElementById('whatsapp-input');
                     if (value.length < 10) {
                         e.preventDefault();
                         return;
                     }
                     var msg = encodeURIComponent('Hello,\n To Know more about our provided services click on given link...\n http://mydc.in/?a=@item.callme-17 \n Thanks & Regards \n @item.customername')
                     e.href = `https://wa.me/91${document.getElementById('whatsapp-input').value}?text=${msg}`;
                     }
                     function handleDirectWhatsappShare(e) {
                         e.href = `whatsapp: \/\/send?text=${window.location.href}`;
                   }
                </script>

