@section scripts{
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script src="~/DigitalCard9/js/materialize.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.0/jquery.validate.js"></script>

    <script>
        $(window).on('load', function ()
        {
        var msg = "@ViewBag.msg";
        if (msg != "") {
            $(".modal-title").html("Alert!")
            $("#msg").html(msg);
            $('#myModal1').modal({ backdrop: 'static', keyboard: false });
        }
    })
    </script>

    <script src="https://www.google.com/recaptcha/api.js" async defer></script>

    <script>
        $(document).ready(function () {
            $("#register").validate({
                ignore: ".ignore",
                rules: {
                    name: {
                        required: true
                    },
                    email: {
                        required: true,
                        email: true
                    },
                    number: {
                        required: true,
                        number: true,
                        minlength: 10
                    },
                    query: {
                        required: true
                    },
                    hiddenRecaptcha: {
                        required: function () {
                            if (grecaptcha.getResponse() == '') {
                                return true;
                            } else {
                                return false;
                            }
                        }
                    }
                },
                messages: {
                    name: {
                        required: "Enter name"
                    },
                    email: {
                        required: "Enter email Id",
                        email: "Enter valid email Id"
                    },
                    number: {
                        required: "Enter phone number",
                        number: "Enter Phone number"
                    },
                    hiddenRecaptcha:
                    {
                        required: "This Field Required"
                    }
                }

            }); //validate
        });
    </script>

    <script>
        function recaptchaCallback() {
            $('#hiddenRecaptcha').valid();
        };
    </script>


    <link href="~/css/JQueryMagnify.css" rel="stylesheet" />
    <script src="~/js/jQueryMagnify.js"></script>
    <script>
        $('[data-magnify]').magnify({
            resizable: true,
            initMaximized: true
        })
    </script>
    <script>
        $(".numbersOnly").keypress(function (e) {
            if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                return false;
            }
        });

        $(".alphabetOnly").keypress(function (e) {
            var keyCode = e.keyCode || e.which;

            //Regex for Valid Characters i.e. Alphabets.
            var regex = /^[a-zA-Z ]*$/;

            //Validate TextBox value against the Regex.
            var isValid = regex.test(String.fromCharCode(keyCode));
            if (!isValid) {
                return false;
            }
        });
    </script>
}
