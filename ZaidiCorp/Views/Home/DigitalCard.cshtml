@model ZaidiCorp.Models.combo
@{
    ViewBag.Title = "DigitalCard";
    Layout = "~/Views/Shared/Layout1.cshtml";
    if (Request.Cookies["msg"] != null)
    {
        ViewBag.msg = Request.Cookies["msg"].Value;
        Request.Cookies["msg"].Expires = DateTime.Now.AddSeconds(0);
    }
}

@foreach (var item in Model.customerinfo)
{
    <title> @item.customername </title>
}

    <style>
        #mobile-bottom-fixed button 
        {
            background: none;
            border: none;
            box-shadow: none;
            color: #fff;
            padding: 10px 5px;
        }

        #mobile-bottom-fixed button i {
            font-size: 120%;
        }

        #mobile-bottom-fixed button span 
        {
            display: block;
        }

        @@media only screen and (max-width:520px) and (min-width:220px)
        {
            #mobile-bottom-fixed button
            {
                font-size: 18px;
            }
        }
    </style>

    <link href="~/DigitalCard/css/bootstrap.css" rel="stylesheet" />
    <link href="~/DigitalCard/css/font-awesome.css" rel="stylesheet" />
