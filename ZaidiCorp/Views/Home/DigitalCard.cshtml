        }

        .mfp-3d-unfold.mfp-ready.mfp-bg {
            opacity: 1 !important;
        }

        @@media (max-width: 767px) {
            .mfp-3d-unfold {
                margin: auto;
                position: fixed !important;
                top: 0 !important;
                left: 0;
                right: 0;
                bottom: 0;
                width: 100%;
                height: 100% !important;
                z-index: 1000;
                -webkit-backface-visibility: hidden;
                -moz-backface-visibility: hidden;
                backface-visibility: hidden;
                overflow-y: auto;
            }
        }
