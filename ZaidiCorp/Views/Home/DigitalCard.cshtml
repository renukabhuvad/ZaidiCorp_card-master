    <style>
        body {
            background: linear-gradient(to right, rgba(68, 147, 228, 1) 50%, rgba(2, 56, 130, 1) 100%);
        }

        #sharenumberBox {
            padding: 15px;
            background: #f1f1f1;
            margin-bottom: 15px;
        }

            #sharenumberBox #sharenumber {
                margin-bottom: 0;
            }

            #sharenumberBox .note {
                margin-bottom: 5px;
                text-align: left;
            }

            #sharenumberBox .title {
                font-size: 110%;
                margin-bottom: 10px;
                font-weight: 500;
            }

            #sharenumberBox button {
                background-color: #25d366;
                padding: 10px;
            }

        hr.popupHr {
            margin: 10px 0;
            border-color: #ddd;
        }

        #sharenumberBox button i {
            color: #fff !important;
            font-size: 150%;
            display: inline-block;
            margin-right: 10px;
            position: relative;
            top: 2px;
        }

        .contact-details.contactDetails a {
            display: table;
        }

            .contact-details.contactDetails a i {
                display: table-cell;
            }

            .contact-details.contactDetails a span {
                word-break: break-all;
                display: table-cell;
                padding-left: 10px;
                vertical-align: middle;
            }

            .contact-details.contactDetails a i::before {
                position: static;
            }
    </style>

    <style>
        .row {
            display: flex;
        }

        @@media only screen and (max-width: 760px) and (min-width: 220px) {
